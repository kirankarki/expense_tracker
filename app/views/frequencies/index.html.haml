%p#notice= notice
%h1 Frequencies
%table
  %thead
    %tr
      %th Name
      %th{:colspan => "3"}
  %tbody
    - @frequencies.each do |frequency|
      %tr
        %td= frequency.name
        %td= link_to 'Show', frequency
        %td= link_to 'Edit', edit_frequency_path(frequency)
        %td= link_to 'Destroy', frequency, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Frequency', new_frequency_path
